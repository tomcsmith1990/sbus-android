<component-metadata>
	<name>slowcar</name>
	<description>Detects slow cars in a traffic stream</description>
	<keywords></keywords>
	<designer>David Ingram</designer>
	<endpoints>
		<endpoint>
			<name>traffic</name>
			<type>#sink</type>
			<message>
				@vehicle
				{
					txt colour
					int speed
				}
			</message>
			<response>!</response>
		</endpoint>
	</endpoints>
</component-metadata>
